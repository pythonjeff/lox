# Alpaca (paper trading)
ALPACA_API_KEY=
ALPACA_API_SECRET=
ALPACA_PAPER=true

# Alpaca market data (if different from trading keys)
ALPACA_DATA_KEY=
ALPACA_DATA_SECRET=

# Options market data feed hint for Alpaca (commonly "opra" for US options)
ALPACA_OPTIONS_FEED=

# OpenAI (required for LLM features)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini

# FRED API (required for macro/economic data)
FRED_API_KEY=

# Financial Modeling Prep API (required when AOT_PRICE_SOURCE=fmp)
FMP_API_KEY=

# Price data source: "fmp" or "alpaca" (default: fmp)
AOT_PRICE_SOURCE=fmp

# Optional: Override default data paths
# AOT_CACHE_DIR=data/cache
# AOT_NAV_SHEET=data/nav_sheet.csv
# AOT_NAV_FLOWS=data/nav_flows.csv
# AOT_NAV_INVESTOR_FLOWS=data/nav_investor_flows.csv
